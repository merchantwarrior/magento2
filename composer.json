{
    "name": "merchantwarrior/magento2-payment",
    "description": "Official Magento2 Plugin to connect to Payment Service Provider Merchant Warrior.",
    "type": "magento2-module",
    "version": "1.0.8",
    "license": "MIT",
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.magento.com/"
        }
    ],
    "require": {
        "php": ">=7.3.0",
        "magento/framework": ">=102.0.3",
        "magento/module-vault": ">=101.1.3",
        "magento/module-payment": ">=100.3.3",
        "magento/module-checkout": ">=100.3.3",
        "magento/module-sales": ">=102.0.3",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.5.0",
        "magento/magento-coding-standard": "*",
        "squizlabs/php_codesniffer": "~3.5.3"
    },
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "MerchantWarrior\\Payment\\": ""
        }
    }
}
